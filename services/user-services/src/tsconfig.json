{
  "compilerOptions":{
     "target": "ES6",
     "esModuleInterop": true,
     "paths": {
       "@core/*": ["core/*"],
       "@domain/*": ["core/domain/*"],
       "@application/*": ["core/application/*"],
       "@infrastructure/*": ["core/infrastructure/*"],
       "@api/*": ["api/*"],
       "@config/*": ["config/*"],
       "@tests/*": ["tests/*"],
       "@utils/*": ["utils/*"]
     }
  }
}
